= (r - g) / delta + 4;\n        }\n        h = (h * 60) % 360; // 确保色相在 0 - 360 范围内\n        if (h < 0) {\n            h += 360;\n        }\n    }\n\n    // 4. 计算亮度（L）\n    const l = (Cmax + Cmin) / 2;\n\n    // 5. 计算饱和度（S）\n    let s = 0;\n    if (delta !== 0) {\n        s = delta / (1 - Math.abs(2 * l - 1));\n    }\n\n    // 返回结果，h, s, l 的值\n    return {\n        h: h,\n        s: s * 100,  // 饱和度的值转换为百分比\n        l: l * 100   // 亮度的值转换为百分比\n    };\n}\n\n"],[0,"inpu"]],"start1":0,"start2":0,"length1":952,"length2":4},{"diffs":[[0,"BLUE)))\n"],[1,"    serial.writeString(\"R: \")\n    serial.writeString(\"\")\n    serial.writeString(\" G:\")\n    serial.writeString(\"\")\n    serial.writeString(\" B:\")\n    serial.writeLine(\"\")\n"],[0,"})\ninput"]],"start1":171,"start2":171,"length1":16,"length2":185},{"diffs":[[0,"(Button."],[1,"A"],[0,"B, funct"]],"start1":372,"start2":372,"length1":16,"length2":17},{"diffs":[[0,"en()\n})\n"],[1,"input.onButtonPressed(Button.B, function () {\n    basic.showString(\"\" + (strip.power()))\n})\n"],[0,"let stri"]],"start1":452,"start2":452,"length1":16,"length2":108},{"diffs":[[0,"r()\n"],[-1,"strip.setBrightness(100)\n"],[0,"stri"]],"start1":737,"start2":737,"length1":33,"length2":8}]},{"type":"edited","filename":"pxt.json","patch":[{"diffs":[[0,"crophone\": \"*\",\n"],[1,"        \"YFMATRIX57\": \"github:yfrobot-tm/pxt-yfrobot-ledmatrix57#afeea9262946a7726834ce64b3bb14cfd266a460\",\n        \"YFSENSORS\": \"github:YFROBOT-TM/pxt-yfrobot-sensors#d491e3455a1398cc4c92676cf1a0f75cfa0c3ff1\",\n        \"YFGesture\": \"github:YFROBOT-TM/pxt-yfrobot-gesture#e301d07f65031ea1d83dee1270fb3bb088d6f164\",\n        \"YFOLED\": \"github:YFROBOT-TM/pxt-yfrobot-oled#3ae45a6b5ccd2eb318c5b3e590abe4f39b3b1d57\",\n"],[0,"        \"neopixe"]],"start1":124,"start2":124,"length1":32,"length2":443},{"diffs":[[0,"184\""],[-1,",\n        \"pxt-YFOLED2\": \"github:YFROBOT-TM/pxt-yfrobot-oled2#V1.0.3\""],[0,"\n   "]],"start1":726,"start2":726,"length1":77,"